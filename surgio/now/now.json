{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "/gateway.js",
      "use": "@now/node",
      "config": {
        "includeFiles": ["provider/**", "template/**", "*.js", "*.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/gateway.js"
    }
  ]
}
